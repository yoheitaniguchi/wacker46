{
    "version": 2,
    "name": "wacker46",
    "builds": [
        { "src": "server.js", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/", "dest": "server.js" },
        { "src": "/webhook", "dest": "server.js" },
        { "src": "/debug", "dest": "server.js" }
    ],
    "env": {
        "CHANNEL_ACCESS_TOKEN": "@channel_access_token",
        "CHANNEL_SECRET": "@channel_secret",
        "UNSPLASHCLIENT_ID":"@unsplashclient_id"
      },
    "github": {
        "enabled": false
    }
}